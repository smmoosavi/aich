{
  "name": "aich",
  "version": "0.0.0",
  "description": "minimal ui construction library for web applications",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/smmoosavi/aich#readme",
  "bugs": {
    "url": "https://github.com/smmoosavi/aich/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smmoosavi/aich.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "pnpx prettier --write .",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^24.7.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "jsdom": "^27.0.0",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
